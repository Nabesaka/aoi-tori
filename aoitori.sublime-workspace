			"contents": "<?php\n/*\nPlugin Name: Add Image Buttons\nDescription: Adds a shop now button and pinterest button to all images inserted via the media uploader\nVersion: 1.0\nAuthor: Paul Robinson\nAuthor URI: http://return-true.com\nLicense: GPL2\n\nNOTE: Could be split into separate files, but kept as one to keep it portable for client.\n\n*/\n\n$attachment_options = array(\n	'image_button_link' => array(\n		'label'			=> __('Image Button Link'),\n		'input'			=> 'text',\n		'helps'			=> __('Where should the button link to? Will not be displayed if left empty.'),\n		'required'		=> false,\n		'application'	=> 'image',\n		'exclusions'	=> array('audio', 'video')\n	)\n);\n\nClass Rt_Custom_Media_Fields {\n\n	private $media_fields = array();\n\n	public function __construct( $fields ) {\n		$this->media_fields = $fields;\n\n		add_action( 'wp_enqueue_scripts', array($this, 'addStyles') );\n		add_action( 'wp_enqueue_scripts', array($this, 'addScripts') );\n		add_filter( 'attachment_fields_to_edit', array($this, 'applyFilter'), 10, 2 );\n		add_filter( 'attachment_fields_to_save', array($this, 'saveFields'), 10, 2 );\n		add_filter( 'image_send_to_editor', array($this, 'modifyInsert'), 10, 8 );\n	}\n\n	public function addStyles() {\n		wp_enqueue_style( 'aib', plugin_dir_url( __FILE__ ) . 'css/aib.css', null, '1.0' );\n	}\n\n	public function addScripts() {\n		wp_enqueue_script( 'aib', plugin_dir_url( __FILE__ ) . 'js/aib.js', array('jquery'), '1.0', true );\n	}\n\n	public function modifyInsert( $html, $id, $caption, $title, $align, $url, $size, $alt ) {\n\n		$o = '';\n		$pinterest = '';\n		$postPerm = get_permalink( get_post_field( 'post_parent', $id) );\n		$media = wp_get_attachment_url($id);\n		$pintIcon = plugin_dir_url( __FILE__ ) . 'images/pinterest.png';\n\n		if( $link = get_post_meta( $id, '_image_button_link', true ) )\n			$pinterest = '<div class=\"shop-button\"><a href=\"' . $link . '\" class=\"shop-link\" target=\"_blank\" rel=\"nofollow\">Shop</a></div>';\n\n		$o = '<div class=\"rt-aib image-container\">' . $html . '<a href=\"http://pinterest.com/pin/create/button/?url=' . urlencode($postPerm) . '&media=' . urlencode($media) . '&description=' . urlencode($title) . '\" class=\"pinterest-share\" target=\"_blank\"><img src=\"' . $pintIcon . '\"></a>' . $pinterest . '</div>';\n\n		return $o;\n	}\n\n	public function applyFilter( $form_fields, $post = null ) {\n		if( !empty( $this->media_fields )) {\n			foreach( $this->media_fields as $field => $values ) {\n				if( preg_match( \"/\" . $values['application'] . \"/\", $post->post_mime_type) && ! in_array( $post->post_mime_type, $values['exclusions'] )) {\n					$meta = get_post_meta( $post->ID, '_' . $field, true);\n\n					switch( $values['input'] ) {\n						default:\n						case 'text':\n							$values['input'] = 'text';\n							break;\n\n						case 'textarea':\n							$values['input'] = 'textarea';\n							break;\n\n						//Other types would be here, but are not needed for this plugin\n					}\n\n					$values['value'] = $meta;\n\n					$form_fields[$field] = $values;\n				}\n			}\n		}\n\n		return $form_fields;\n	}\n\n	public function saveFields( $post, $attachment ) {\n		if( !empty( $this->media_fields ) ) {\n			foreach( $this->media_fields as $field => $values ) {\n				if( isset( $attachment[$field] ) ) {\n					if( strlen( trim( $attachment[$field] ) ) == 0 ) {\n						$post['errors'][$field]['errors'][] = __( $values['error_text'] );\n					} else {\n						update_post_meta( $post['ID'], '_' . $field, $attachment[$field]);\n					}\n				} else {\n					delete_post_meta( $post['ID'], '_' . $field);\n				}\n			}\n		}\n\n		return $post;\n	}\n}\n\n$rtcmf = new Rt_Custom_Media_Fields( $attachment_options );\n\n/**\n * Helper function for adding a shop & pinterest button to a featured image, use instead of WordPress' the_post_thumbnail();\n * Function prefix is to prevent function conflicts\n */\n\nfunction rt_post_thumbnail_with_buttons($size = 'thumbnail', $attr = array(),  $id = null) {\n\n	if(is_null($id))\n		global $post;\n\n	$tid = get_post_thumbnail_id( $post->ID );\n\n	$o = '';\n	$pinterest = '';\n	$postPerm = get_permalink( $post->ID );\n	$media = wp_get_attachment_url($tid);\n	$pintIcon = plugin_dir_url( __FILE__ ) . 'images/pinterest.png';\n	$html = wp_get_attachment_image($tid, $size, false, $attr);\n\n	if( $link = get_post_meta( $tid, '_image_button_link', true ) )\n		$pinterest = '<div class=\"shop-button\"><a href=\"' . $link . '\" class=\"shop-link\" target=\"_blank\" rel=\"nofollow\">Shop</a></div>';\n\n	$o = '<div class=\"rt-aib image-container\">' . $html . '<a href=\"http://pinterest.com/pin/create/button/?url=' . urlencode($postPerm) . '&media=' . urlencode($media) . '&description=' . urlencode(esc_attr(get_the_title($tid))) . '\" class=\"pinterest-share\" target=\"_blank\"><img src=\"' . $pintIcon . '\"></a>' . $pinterest . '</div>';\n\n	echo $o;\n}",
